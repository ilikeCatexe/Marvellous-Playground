local VLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/kickTh/New-Ui/main/VLib.txt"))()
MAINTTL = "PLANG HUB" 
local win = VLib:Window("MarvellousPlayground", Color3.fromRGB(196, 40, 28))
local Main = win:Tab("Main")

_G.infc = false
_G.autoblock = false

Main:Button("Destroy Gui",function()
game.CoreGui["Library"]:Destroy()
game.CoreGui["Plang Hub"]:Destroy()
end)
local Main = win:Tab("Gamepass Characters")

local function changeplayer(char)
    game:GetService("ReplicatedStorage").SelectCharacter.ChangeChar:FireServer(char)
end

Main:Button("GigaChad",function()
    changeplayer(game:GetService("ReplicatedStorage").SelectCharacter.GigaChad)
end)

Main:Button("Chadette",function()
    changeplayer(game:GetService("ReplicatedStorage").Skins.GigaChad.Skin1)
end)

Main:Button("Saitama",function()
    changeplayer(game:GetService("ReplicatedStorage").Skins.GigaChad.Skin2)
end)

Main:Button("All Might",function()
    changeplayer(game:GetService("ReplicatedStorage").Skins.GigaChad.Skin3)
end)

Main:Button("IShowSpeed",function()
    changeplayer(game:GetService("ReplicatedStorage").SelectCharacter.Speed)
end)
local Main = win:Tab("Character")

Main:Button("Walter",function()
    changeplayer(game:GetService("ReplicatedStorage").SelectCharacter.Walter)
end)

Main:Button("Springtrap",function()
    changeplayer(game:GetService("ReplicatedStorage").SelectCharacter.Springtrap)
end)

Main:Button("Zoom",function()
    changeplayer(game:GetService("ReplicatedStorage").SelectCharacter.Zoom)
end)

local Main = win:Tab("Main funtion")
local infcxd = {
    [1] = game:GetService("Players").LocalPlayer
}

Main:Button("Stop Inf Counter",function()
    _G.infc = false
end)

Main:Button("Inf Counter (Work Only Saitama)",function()
_G.infc = true
wait(0)
while _G.infc do wait()
game:GetService("Players").LocalPlayer.Character.Saitama.CounterActivate:FireServer(unpack(infcxd))
end
end)

Main:Button("Stop Auto Block",function()
    _G.autoblock = false
end)

Main:Button("Auto Block",function()
    local block = {
        [1] = game:GetService("Players").LocalPlayer
    }
    _G.autoblock = true
    wait(0)
    while _G.autoblock do wait()
    game:GetService("ReplicatedStorage").Characters.Combat.Remotes.Blocking:FireServer(unpack(block))
    wait(1)
    end
end)

